NET "ADV_P<22>" LOC = "P50" | IOSTANDARD = LVCMOS25;
NET "ADV_P<23>" LOC = "P47" | IOSTANDARD = LVCMOS25;

NET "OUTPUT<0>" LOC = "P116" | IOSTANDARD = LVTTL | DRIVE = 2 | SLEW = SLOW;
NET "OUTPUT<1>" LOC = "P117" | IOSTANDARD = LVTTL | DRIVE = 2 | SLEW = SLOW;
NET "OUTPUT<2>" LOC = "P118" | IOSTANDARD = LVTTL | DRIVE = 2 | SLEW = SLOW;
NET "OUTPUT<3>" LOC = "P119" | IOSTANDARD = LVTTL | DRIVE = 2 | SLEW = SLOW;
NET "OUTPUT<4>" LOC = "P120" | IOSTANDARD = LVTTL | DRIVE = 2 | SLEW = SLOW;
